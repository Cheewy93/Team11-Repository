package beans;

import javax.faces.bean.SessionScoped;

import model.User;

@SessionScoped
public class IndexManagedBean {
	
	private int role_admin = User.role_admin;
	private int role_owner = User.role_other;
	public int getRole_admin() {
		return role_admin;
	}
	public void setRole_admin(int role_admin) {
		this.role_admin = role_admin;
	}
	public int getRole_owner() {
		return role_owner;
	}
	public void setRole_owner(int role_owner) {
		this.role_owner = role_owner;
	}

}
